@model IEnumerable<BarcodeCombined.Models.stock_export>

<h2 id="header">ProcessDetails</h2>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Stock_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stock_description)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Bar_code)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Stock_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock_description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bar_code)
            </td>
        </tr>
    }
</table>

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <button type="button" class="btn-primary" onclick="location.href='@Url.Action("Index","Home")'"> New Search</button>
    </div>
</div>
